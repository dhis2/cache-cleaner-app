{
    "name": "cache-cleaner-app",
    "version": "1.0.0",
    "description": "",
    "license": "BSD-3-Clause",
    "private": true,
    "scripts": {
        "build": "d2-app-scripts build",
        "format": "yarn format:js && yarn format:text",
        "format:staged": "yarn format:js --staged && yarn format:text --staged",
        "format:js": "d2-style js apply",
        "format:text": "d2-style text apply",
        "lint": "yarn lint:js && yarn lint:text",
        "lint:js": "d2-style js check",
        "lint:text": "d2-style text check",
        "start": "d2-app-scripts start",
        "test": "d2-app-scripts test",
        "cypress:start": "BROWSER=none REACT_APP_DHIS2_BASE_URL=https://debug.dhis2.org/dev REACT_APP_DHIS2_API_VERSION=37 yarn start",
        "cypress:open": "d2-utils-cypress open --appStart 'yarn cypress:start'",
        "cypress:run": "d2-utils-cypress run --appStart 'yarn cypress:start'",
        "cypress:capture": "d2-utils-cypress run --capture --appStart  'yarn cypress:start' --serverMinorVersion 37",
        "cypress:stub": "d2-utils-cypress run --stub --appStart  'yarn cypress:start' --serverMinorVersion 37"
    },
    "devDependencies": {
        "@dhis2/cli-app-scripts": "^6.1.3",
        "@dhis2/cli-style": "^7.3.0",
        "@dhis2/cli-utils-cypress": "^8.0.0-alpha.3",
        "@testing-library/react-hooks": "^3.4.1",
        "fake-indexeddb": "^3.0.2",
        "react-test-renderer": "^16.13.1"
    },
    "dependencies": {
        "@dhis2/app-runtime": "^2.8.0",
        "@dhis2/cypress-commands": "^8.0.0-alpha.3",
        "@dhis2/cypress-plugins": "^8.0.0-alpha.3",
        "@dhis2/d2-i18n": "^1.1.0",
        "@dhis2/prop-types": "^1.6.4",
        "@dhis2/ui": "^6.6.3",
        "final-form": "^4.19.1"
    }
}
